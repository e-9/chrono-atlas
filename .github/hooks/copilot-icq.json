{
  "hooks": {
    "errorOccurred": [
      {
        "bash": "/usr/local/bin/copilot-icq-hook errorOccurred",
        "powershell": "/usr/local/bin/copilot-icq-hook errorOccurred",
        "timeoutSec": 5,
        "type": "command"
      }
    ],
    "postToolUse": [
      {
        "bash": "/usr/local/bin/copilot-icq-hook postToolUse",
        "powershell": "/usr/local/bin/copilot-icq-hook postToolUse",
        "timeoutSec": 5,
        "type": "command"
      }
    ],
    "preToolUse": [
      {
